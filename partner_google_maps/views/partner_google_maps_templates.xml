<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<!-- inherit from web assets backend -->
	<template id="assets_backend" name="partner_google_maps assets"
		inherit_id="web.assets_backend">
		<xpath expr="." position="inside">
			<link rel="stylesheet" href="/partner_google_maps/static/src/css/custom_view.css" />
			
			<script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?v=3"></script>
			<!-- <script type="text/javascript" src="/partner_google_maps/static/src/js/custom_view.js"></script> -->
			<script type="text/javascript" src="/partner_google_maps/static/src/js/partner_google_maps.js"></script>
		</xpath>
	</template>
	
</odoo>